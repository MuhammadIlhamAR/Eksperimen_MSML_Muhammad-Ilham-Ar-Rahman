name: TugasAkhir-MSML
conda_env: conda.yaml

entry_points:
  train:
    parameters:
      input_file: {type: str, default: "PRSA_Data_Aotizhongxin_preprocessing.csv"}
      n_estimators: {type: int, default: 100}
      random_state: {type: int, default: 42}
      test_size: {type: float, default: 0.2}
      mlflow_tracking_uri: {type: str, default: ""}
    command: "python modelling.py {input_file} {n_estimators} {random_state} {test_size} {mlflow_tracking_uri}"
