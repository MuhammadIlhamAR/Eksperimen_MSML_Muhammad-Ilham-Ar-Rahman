name: TugasAkhir-MSML
conda_env: conda.yaml
entry_points:
  main:
    parameters:
      input_file: {type: str, default: "PRSA_Data_Aotizhongxin_preprocessing.csv"}
      n_estimators: {type: int, default: 200}
      max_depth: {type: int, default: 20}
      min_samples_split: {type: int, default: 2}
      mlflow_tracking_uri: {type: str, default: "https://dagshub.com/m.ilham2408/my-first-repo.mlflow"}
    command: "python modelling.py --input_file {input_file} --n_estimators {n_estimators} --max_depth {max_depth} --min_samples_split {min_samples_split} --mlflow_tracking_uri {mlflow_tracking_uri}"
